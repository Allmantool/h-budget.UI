{
    "name": "h-budget",
    "version": "0.5.68",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "nx serve --watch --open",
        "start-prod": "nx serve --watch --open --port=4300 --configuration=production",
        "build": "nx build --verbose",
        "build-prod": "nx build h-budget --configuration=production --verbose",
        "watch": "nx build --watch --configuration=development",
        "test": "nx test --code-coverage --verbose",
        "test-headless-ci-only": "nx test --browsers=ChromiumNoSandbox",
        "test-headless": "nx test --watch=false --code-coverage --source-map=false --browsers=ChromiumNoSandbox",
        "lint:fix": "nx lint -- --fix --verbose",
        "lint-css": "stylelint src/**/*.css",
        "prettier": "prettier --write src/**/*.{js,ts,css}",
        "eslint": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx src",
        "eslint:dump": "eslint --print-config ./.eslintrc.json",
        "sonar-scanner": "sonar-scanner",
        "clean": "rimraf node_modules",
        "reinstall": "npm run clean && npm install",
        "rebuild": "npm run clean && npm install && rimraf dist && npm run build-prod",
        "build-image": "docker build -t nginx-app-docker-image .",
        "remove-image": "docker rmi nginx-app-docker-image",
        "run-container": "docker run --name nginx-app-docker-container -d -p 8080:80 nginx-app-docker-image",
        "remove-container": "docker rm -f nginx-app-docker-container",
        "containerize": "npm run build-image && npm run run-container",
        "recontainerize": "npm run remove-container && npm run build-image && npm run run-container"
    },
    "dependencies": {
        "@angular-devkit/build-angular": "^18.2.10",
