    "@nrwl/angular": "^19.8.4",
    "@nrwl/cli": "^15.9.3",
    "@nrwl/devkit": "^19.8.4",
    "@nrwl/workspace": "^19.8.4",
    "@nx/angular": "^20.1.4",
    "@nx/linter": "^19.8.4",
    "@nx/workspace": "^20.1.4",
    "@schematics/angular": "^18.2.12",
    "@types/jasmine": "~5.1.5",
