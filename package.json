        "@ngxs/devtools-plugin": "^18.1.4",
        "@nx/angular": "^19.8.4",
        "@nx/linter": "^19.8.4",
        "@nx/workspace": "^19.8.4",
        "@schematics/angular": "^18.2.10",
        "@types/jasmine": "~5.1.4",
        "@types/lodash": "^4.17.12",
