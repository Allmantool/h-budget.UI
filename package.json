{
  "name": "h-budget",
  "version": "0.168.68",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "nx serve --watch --open --ssl true --configuration=developments",
    "start-prod": "nx serve --watch --open --port=4300 --configuration=production",
    "build": "nx build --configuration=development --verbose && npm run sentry:sourcemaps",
    "build-prod": "nx build h-budget --configuration=production --verbose",
    "watch": "nx build --watch --configuration=development",
    "test": "nx test --code-coverage --verbose",
    "test-headless-ci-only": "nx test --browsers=ChromiumNoSandbox",
    "test-headless": "nx test --watch=false --code-coverage --source-map=false --browsers=ChromiumNoSandbox",
    "lint": "nx lint",
    "lint-fix": "nx lint --fix --verbose",
    "lint-css": "stylelint src/**/*.css",
    "prettier": "prettier --write src/**/*.{js,ts,css}",
    "eslint": "eslint --cache",
    "eslint-fix": "eslint --cache --fix --ext .js,.ts src",
    "eslint-dump": "eslint --print-config ./.eslintrc.json",
    "sonar-scanner": "sonar-scanner",
    "clean": "npm i rimraf && npm cache clean --force && npm cache verify && npm dedupe && npm prune && rimraf node_modules && rimraf package-lock.json",
    "reinstall": "npm run clean && npm i",
    "rebuild": "npm run clean && npm i && npm dedupe && npm run build",
    "rebuild-prod": "npm run clean && npm i --omit=dev && rimraf dist && npm run build-prod",
    "major-update": "npm i npm-check-updates && ncu -u",
    "minor-update": "npm i npm-check-updates && ncu --peer -u",
    "soft-update": "npm run rebuild && npm run minor-update && npm i && npm update && npm i && npm ci",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org home-t2h --project accounting-api ./ && sentry-cli sourcemaps upload --org home-t2h --project accounting-api ./",
    "hard-update": "npm run rebuild && npm run major-update && npm i && npm update && npm i && npm ci"
  },
  "dependencies": {
    "@angular/animations": "^20.1.2",
    "@angular/cdk": "^20.1.2",
    "@angular/common": "^20.1.2",
    "@angular/compiler": "^20.1.2",
    "@angular/core": "^20.1.2",
    "@angular/forms": "^20.1.2",
    "@angular/material": "^20.1.2",
    "@angular/platform-browser": "^20.1.2",
    "@angular/platform-browser-dynamic": "^20.1.2",
    "@angular/router": "^20.1.2",
    "@dynamic-mapper/angular": "^1.1.1",
    "@dynamic-mapper/mapper": "^1.10.4",
    "@ngxs/devtools-plugin": "^20.1.0",
    "@ngxs/logger-plugin": "^20.1.0",
    "@ngxs/store": "^20.1.0",
    "@nrwl/linter": "^19.8.4",
    "@sentry/angular": "^9.40.0",
    "@sentry/cli": "^2.50.0",
    "@sentry/tracing": "^7.120.3",
    "@types/lodash": "^4.17.20",
    "@types/uuid": "^10.0.0",
    "apexcharts": "^5.2.0",
    "date-fns": "^4.1.0",
    "downsample": "^1.4.0",
    "flag-icons": "^7.5.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "material-design-icons": "^3.0.1",
    "ng-apexcharts": "~1.16.0",
    "npm-check-updates": "^18.0.1",
    "rimraf": "^6.0.1",
    "rxjs": "~7.8.2",
    "stylelint": "^16.22.0",
    "ts-evaluator": "^2.0.0",
    "ts-simple-nameof": "^1.3.1",
    "tslib": "^2.8.1",
    "typescript-guid": "^1.0.3",
    "uuid": "^11.1.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.1",
    "@angular-eslint/builder": "20.1.1",
    "@angular-eslint/eslint-plugin": "20.1.1",
    "@angular-eslint/eslint-plugin-template": "20.1.1",
    "@angular-eslint/schematics": "20.1.1",
    "@angular-eslint/template-parser": "20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nx/angular": "^21.3.1",
    "@nx/linter": "^19.8.4",
    "@nx/workspace": "^21.3.1",
    "@schematics/angular": "^20.1.1",
    "@types/jasmine": "~5.1.8",
    "@types/node": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "jasmine-core": "~5.9.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-sonarqube-reporter": "^1.4.0",
    "lint-staged": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "nx": "21.3.1",
    "nx-cloud": "19.1.0",
    "prettier": "^3.6.2",
    "sonarqube-scanner": "^4.3.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "~5.8.3"
  },
  "nx": {}
}
