{
  "name": "h-budget",
  "version": "0.141",
  "scripts": {
    "ng": "ng",
    "start": "nx serve --watch --open",
    "build": "nx build",
    "build-prod": "nx build --configuration=production",
    "watch": "nx build --watch --configuration=development",
    "test": "nx test --code-coverage",
    "test-headless-ci-only": "nx test --browsers=ChromiumNoSandbox",
    "test-headless": "nx test --watch=false --code-coverage --source-map=false --browsers=ChromiumNoSandbox",
    "lint": "nx lint --verbose",
    "lint-css": "stylelint src/**/*.css",
    "prettier": "prettier --write src/**/*.{js,ts,css}",
    "eslint": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "sonar-scanner": "sonar-scanner",
    "prepare": "cd .. && husky install Angular-app/.husky",
    "clean": "rmdir /s /q node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && rmdir /s /q dist && npm run build --prod",
    "build-image": "docker build -t nginx-example-app-docker-image .",
    "remove-image": "docker rmi nginx-example-app-docker-image",
    "run-container": "docker run --name nginx-example-app-docker-container -d -p 8080:80 nginx-example-app-docker-image",
    "remove-container": "docker rm -f nginx-example-app-docker-container",
    "containerize": "npm run build-image && npm run run-container",
    "recontainerize": "npm run remove-container && npm run build-image && npm run run-container"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.11",
    "@angular/cdk": "^16.2.11",
    "@angular/common": "^16.2.11",
    "@angular/compiler": "^16.2.11",
    "@angular/core": "^16.2.11",
    "@angular/forms": "^16.2.11",
    "@angular/material": "^16.2.11",
    "@angular/platform-browser": "^16.2.11",
    "@angular/platform-browser-dynamic": "^16.2.11",
    "@angular/router": "^16.2.11",
    "@dynamic-mapper/angular": "^1.1.1",
    "@dynamic-mapper/mapper": "^1.10.2",
    "@ngxs/logger-plugin": "^3.8.1",
    "@ngxs/store": "^3.8.1",
    "apexcharts": "^3.44.0",
    "codelyzer": "^6.0.2",
    "date-fns": "^2.30.0",
    "flag-icon-css": "^4.1.7",
    "flag-icons": "^6.11.2",
    "immer": "^10.0.3",
    "lodash": "^4.17.21",
    "material-design-icons": "^3.0.1",
    "ng-apexcharts": "^1.8.0",
    "rxjs": "~7.8.1",
    "stylelint": "^15.11.0",
    "ts-evaluator": "^1.2.0",
    "ts-simple-nameof": "^1.3.1",
    "tslib": "^2.6.2",
    "typescript-guid": "^1.0.3",
    "uuid": "^9.0.1",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.9",
    "@angular-devkit/core": "^16.2.9",
    "@angular-devkit/schematics": "^16.2.9",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.9",
    "@angular/compiler-cli": "^16.2.11",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@nx/angular": "17.0.2",
    "@nx/linter": "17.0.2",
    "@nx/workspace": "17.0.2",
    "@schematics/angular": "^16.2.9",
    "@types/jasmine": "~5.1.1",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-sonarqube-reporter": "^1.4.0",
    "lint-staged": "^15.0.2",
    "npm-run-all": "^4.1.5",
    "nx": "17.0.2",
    "nx-cloud": "16.5.2",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "sonarqube-scanner": "^3.1.0",
    "stylelint-prettier": "^4.0.2",
    "typescript": "~5.1.6"
  }
}
