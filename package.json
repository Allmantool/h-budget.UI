{
    "name": "h-budget",
    "version": "0.1.3",
    "scripts": {
        "ng": "ng",
        "start": "nx serve --watch --open",
        "start-prod": "nx serve --watch --open --port=4300 --configuration=production",
        "build": "nx build",
        "build-prod": "nx build --configuration=production",
        "watch": "nx build --watch --configuration=development",
        "test": "nx test --code-coverage",
        "test-headless-ci-only": "nx test --browsers=ChromiumNoSandbox",
        "test-headless": "nx test --watch=false --code-coverage --source-map=false --browsers=ChromiumNoSandbox",
        "lint:fix": "nx lint -- --fix --verbose",
        "lint-css": "stylelint src/**/*.css",
        "prettier": "prettier --write src/**/*.{js,ts,css}",
        "eslint": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx src",
        "eslint:dump": "eslint --print-config ./.eslintrc.json",
        "sonar-scanner": "sonar-scanner",
        "prepare": "cd .. && husky install UI/.husky",
        "clean": "rmdir /s /q node_modules",
        "reinstall": "npm run clean && npm install",
        "rebuild": "npm run clean && npm install && rmdir /s /q dist && npm run build --prod",
        "build-image": "docker build -t nginx-example-app-docker-image .",
        "remove-image": "docker rmi nginx-example-app-docker-image",
        "run-container": "docker run --name nginx-example-app-docker-container -d -p 8080:80 nginx-example-app-docker-image",
        "remove-container": "docker rm -f nginx-example-app-docker-container",
        "containerize": "npm run build-image && npm run run-container",
        "recontainerize": "npm run remove-container && npm run build-image && npm run run-container"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.2.4",
        "@angular/cdk": "^17.2.2",
        "@angular/common": "^17.2.4",
        "@angular/compiler": "^17.2.4",
        "@angular/core": "^17.2.4",
        "@angular/forms": "^17.2.4",
        "@angular/material": "^17.2.2",
        "@angular/platform-browser": "^17.2.4",
        "@angular/platform-browser-dynamic": "^17.2.4",
        "@angular/router": "^17.2.4",
        "@dynamic-mapper/angular": "^1.1.1",
        "@dynamic-mapper/mapper": "^1.10.4",
        "@ngxs/logger-plugin": "^3.8.2",
        "@ngxs/store": "^3.8.2",
        "apexcharts": "^3.46.0",
        "date-fns": "^3.3.1",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "flag-icons": "^7.2.0",
        "immer": "^10.0.3",
        "lodash": "^4.17.21",
        "material-design-icons": "^3.0.1",
        "ng-apexcharts": "^1.9.0",
        "rxjs": "~7.8.1",
        "stylelint": "^16.2.1",
        "ts-evaluator": "^1.2.0",
        "ts-simple-nameof": "^1.3.1",
        "tslib": "^2.6.2",
        "typescript-guid": "^1.0.3",
        "uuid": "^9.0.1",
        "zone.js": "^0.14.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^17.2.3",
        "@angular-devkit/core": "^17.2.3",
        "@angular-devkit/schematics": "^17.2.3",
        "@angular-eslint/builder": "17.2.1",
        "@angular-eslint/eslint-plugin": "17.2.1",
        "@angular-eslint/eslint-plugin-template": "17.2.1",
        "@angular-eslint/schematics": "17.2.1",
        "@angular-eslint/template-parser": "17.2.1",
        "@angular/cli": "^17.2.3",
        "@angular/compiler-cli": "^17.2.4",
        "@commitlint/cli": "^19.0.3",
        "@commitlint/config-conventional": "^19.0.3",
        "@ngxs/devtools-plugin": "^3.8.2",
        "@nx/angular": "18.0.7",
        "@nx/linter": "18.0.7",
        "@nx/workspace": "18.0.7",
        "@schematics/angular": "^17.2.3",
        "@types/jasmine": "~5.1.4",
        "@types/lodash": "^4.14.202",
        "@types/node": "^20.11.25",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "husky": "^9.0.11",
        "jasmine-core": "~5.1.2",
        "karma": "^6.4.3",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "karma-sonarqube-reporter": "^1.4.0",
        "lint-staged": "^15.2.2",
        "npm-run-all": "^4.1.5",
        "nx": "18.0.7",
        "nx-cloud": "18.0.0",
        "prettier": "^3.2.5",
        "sonarqube-scanner": "^3.3.0",
        "stylelint-prettier": "^5.0.0",
        "typescript": "~5.3.3"
    }
}
