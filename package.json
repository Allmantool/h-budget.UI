{
    "name": "h-budget",
    "version": "0.0.14",
    "scripts": {
        "ng": "ng",
        "start": "nx serve --watch --open",
        "start-prod": "nx serve --watch --open --port=4300 --configuration=production",
        "build": "nx build",
        "build-prod": "nx build --configuration=production",
        "watch": "nx build --watch --configuration=development",
        "test": "nx test --code-coverage",
        "test-headless-ci-only": "nx test --browsers=ChromiumNoSandbox",
        "test-headless": "nx test --watch=false --code-coverage --source-map=false --browsers=ChromiumNoSandbox",
        "lint:fix": "nx lint -- --fix --verbose",
        "lint-css": "stylelint src/**/*.css",
        "prettier": "prettier --write src/**/*.{js,ts,css}",
        "eslint": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx src",
        "eslint:dump": "eslint --print-config ./.eslintrc.json",
        "sonar-scanner": "sonar-scanner",
        "prepare": "cd .. && husky install UI/.husky",
        "clean": "rmdir /s /q node_modules",
        "reinstall": "npm run clean && npm install",
        "rebuild": "npm run clean && npm install && rmdir /s /q dist && npm run build --prod",
        "build-image": "docker build -t nginx-example-app-docker-image .",
        "remove-image": "docker rmi nginx-example-app-docker-image",
        "run-container": "docker run --name nginx-example-app-docker-container -d -p 8080:80 nginx-example-app-docker-image",
        "remove-container": "docker rm -f nginx-example-app-docker-container",
        "containerize": "npm run build-image && npm run run-container",
        "recontainerize": "npm run remove-container && npm run build-image && npm run run-container"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.1.2",
        "@angular/cdk": "^18.1.2",
        "@angular/common": "^18.1.2",
        "@angular/compiler": "^18.1.2",
        "@angular/core": "^18.1.2",
        "@angular/forms": "^18.1.2",
        "@angular/material": "^18.1.2",
        "@angular/platform-browser": "^18.1.2",
        "@angular/platform-browser-dynamic": "^18.1.2",
        "@angular/router": "^18.1.2",
        "@dynamic-mapper/angular": "^1.1.1",
        "@dynamic-mapper/mapper": "^1.10.4",
        "@ngxs/logger-plugin": "^18.0.0",
        "@ngxs/store": "^18.0.0",
        "apexcharts": "^3.51.0",
        "date-fns": "^3.6.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "flag-icons": "^7.2.3",
        "immer": "^10.1.1",
        "lodash": "^4.17.21",
        "material-design-icons": "^3.0.1",
        "ng-apexcharts": "^1.11.0",
        "rxjs": "~7.8.1",
        "stylelint": "^16.7.0",
        "ts-evaluator": "^1.2.0",
        "ts-simple-nameof": "^1.3.1",
        "tslib": "^2.6.3",
        "typescript-guid": "^1.0.3",
        "uuid": "^10.0.0",
        "zone.js": "^0.14.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.1.2",
        "@angular-devkit/core": "^18.1.2",
        "@angular-devkit/schematics": "^18.1.2",
        "@angular-eslint/builder": "18.1.0",
        "@angular-eslint/eslint-plugin": "18.1.0",
        "@angular-eslint/eslint-plugin-template": "18.1.0",
        "@angular-eslint/schematics": "18.1.0",
        "@angular-eslint/template-parser": "18.1.0",
        "@angular/cli": "^18.1.2",
        "@angular/compiler-cli": "^18.1.2",
        "@commitlint/cli": "^19.3.0",
        "@commitlint/config-conventional": "^19.2.2",
        "@ngxs/devtools-plugin": "^18.0.0",
        "@nx/angular": "19.5.3",
        "@nx/linter": "19.5.3",
        "@nx/workspace": "19.5.3",
        "@schematics/angular": "^18.1.2",
        "@types/jasmine": "~5.1.4",
        "@types/lodash": "^4.17.7",
        "@types/node": "^20.14.12",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^7.17.0",
        "@typescript-eslint/parser": "^7.17.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^9.1.3",
        "jasmine-core": "~5.2.0",
        "karma": "^6.4.3",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.1.0",
        "karma-sonarqube-reporter": "^1.4.0",
        "lint-staged": "^15.2.7",
        "npm-run-all": "^4.1.5",
        "nx": "19.5.3",
        "nx-cloud": "19.0.0",
        "prettier": "^3.3.3",
        "sonarqube-scanner": "^4.0.1",
        "stylelint-prettier": "^5.0.2",
        "typescript": "~5.5.4"
    }
}
